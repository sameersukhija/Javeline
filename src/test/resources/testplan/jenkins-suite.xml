<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression Suite" verbose="1" parallel="false" time-out="80000">

  <listeners>
    <listener class-name="com.lumata.e4o.testing.common.TestNGReportListener"/>
  </listeners>
  
  <parameter name="e4oReleaseParam" value="2.4.1.20.0" />  
  <parameter name="networkEnvironmentParams" value="{ 'envFile':'e4o_qa2_ita', 'schemaGlobal': 'global', 'schemaMaster': 'tenantMaster', 'schemaReport': 'tenantReport', 'guiServer':'actrule', 'guiUser':'superman' }"/>
  <parameter name="seleniumWebDriverParams" value="{ 'type':'local', 'local': { 'browserName':'firefox', 'firefox': {} }, 'remote': { 'selenium_hub':'http://ci.lumata.int/wd/hub', 'capability': { 'platform': 'LINUX', 'browserName': 'chrome' } } }"/> 
  
  <test name="Regression suite to test Jenkins Job">

    <classes>

      <class name="com.lumata.e4o.regressions.gui.TestJenkins">
        <!-- methods>
          <include name="configureDMNotifications"/>
        </methods -->
      </class>

    </classes>

  </test>
</suite>
