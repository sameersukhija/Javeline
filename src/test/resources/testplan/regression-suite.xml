<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression Suite" verbose="1" parallel="false">

 <listeners>
    <listener class-name="com.lumata.e4o.testing.common.TestNGReportListener"/>
  </listeners>
	<test name="Run Expired Data">
   <parameter name="environment" value="e4o_regression"/>
	<parameter name="tenant" value="tenantMaster"/>
	<classes>
		<class name="com.lumata.e4o.scheduling.scheduledtasks.ExpiredDataTask">
		<methods>
			<include name="exec_ExpiredData_task"/>
		</methods>
		</class>
	</classes>
	</test>
	
	<parameter name="e4oReleaseParam" value="2.4.2.8.0" />
  
  <parameter name="networkEnvironmentParams" value="{ 'envFile':'e4o_regression', 'schemaGlobal': 'global', 'schemaMaster': 'tenantMaster', 'schemaReport': 'tenantReport', 'guiServer':'actrule1', 'guiUser':'superman' }"/>
  <!-- selenium_driver_type: local or remote -->  
  <!-- 	selenium_capability
  		platform -> {WINDOWS|XP|VISTA|MAC|LINUX|UNIX|ANDROID} ( note: only WINDOWS and LINUX are available in our environments )
  		browserName -> {android|chrome|firefox|htmlunit|internet explorer|iPhone|iPad|opera|safari}
  		version -> the version of the used browser
  -->
  <!-- parameter name="seleniumWebDriverParams" value="{ 'type':'remote', 'local': { 'browserName':'firefox', 'firefox': {} }, 'remote': { 'selenium_hub':'http://192.168.56.152:4444/wd/hub', 'capability': { 'platform': 'LINUX', 'browserName': 'firefox' } } }"/ -->  
   <parameter name="seleniumWebDriverParams" value="{ 'type':'remote', 'local': { 'browserName':'firefox', 'firefox': {} }, 'remote': { 'selenium_hub':'http://ci.lumata.int/wd/hub', 'capability': { 'platform': 'LINUX', 'browserName': 'firefox'} } }"/>  
